    "coveralls": "^3.1.1",
    "dns-packet": "^5.6.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "markdownlint-cli": "^0.41.0",
    "mocha": "^10.4.0",
