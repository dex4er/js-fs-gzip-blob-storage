    ".": {
      "default": "./lib/fs-gzip-blob-storage.js",
      "typings": "./lib/fs-gzip-blob-storage.d.ts"
    }
  },
  "main": "lib/fs-gzip-blob-storage.js",
  "typings": "lib/fs-gzip-blob-storage.d.ts",
  "repository": {
    "type": "git",
