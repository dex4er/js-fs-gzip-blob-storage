  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "fs-blob-storage": "^4.1.0",
    "pumpify": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@tsconfig/node16": "^16.1.3",
